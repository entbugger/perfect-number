# EditorConfig helps developers define and maintain consistent
# coding styles between different editors and IDEs
# editorconfig.org

root = true


[*.{java,xml}]
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false
indent_style = space
indent_size = 4

[*.sh]
end_of_line = lf

[*.{diff,md}]
trim_trailing_whitespace = false
